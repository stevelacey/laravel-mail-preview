{
    "name": "stevelacey/laravel-mail-preview",
    "description": "Adds a 'preview' mail driver for testing emails in Laravel.",
    "keywords": ["laravel", "mail"],
    "homepage": "https://github.com/stevelacey/laravel-preview-mail",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Said",
            "email": "theMohamedSaid@gmail.com"
        },
        {
            "name": "Steve Lacey",
            "email": "steve@stevelacey.net"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": ">=5.5",
        "illuminate/mail": ">=5.5",
        "illuminate/filesystem": ">=5.5"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Themsaid\\MailPreview\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Themsaid\\MailPreview\\MailPreviewServiceProvider"
            ]
        }
    }
}
